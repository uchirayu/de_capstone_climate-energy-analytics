{
  "expectation_suite_name": "silver_openweather",
  "expectations": [
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": { "column": "temperature" }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": { "column": "city" }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": { "column": "weather_status" }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": { "column": "ingestion_timestamp" }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": { "column": "windspeed" }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "temperature",
        "min_value": -20,
        "max_value": 80
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "humidity",
        "min_value": 0,
        "max_value": 100
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": { "column": "city" }
    }
  ]
}
